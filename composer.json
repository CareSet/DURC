{
    "name": "careset/durc",
    "description": "DURC is reverse CRUD",
    "type": "library",
    "license": "MIT",
     "keywords": [
		"laravel",
		"crud",
		"orm",
		"mysql",
		"mariadb"
    ],
    "homepage": "https://github.com/CareSet/DURC",
    "authors": [
        {
            "name": "Fred Trotter",
            "email": "fred.trotter@gmail.com",
            "homepage": "https://www.careset.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php" : "^8.1",
        "erusev/parsedown": "^1.7",
        "zordius/lightncandy": "dev-master"
    },
    "psr-4": {
            "App\\": "app/",
            "CareSet\\DURC\\": "packages/careset/durc/src"
    },
    "autoload": {
	"psr-4": {
		"CareSet\\DURC\\": "src/"
	}
    },
    "post-update-cmd": [
      "php artisan vendor:publish --tag=public"
    ],
	"extra": {
		"laravel": {
			"providers": [
				"CareSet\\DURC\\DURCServiceProvider"
			]
		}
	},
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
